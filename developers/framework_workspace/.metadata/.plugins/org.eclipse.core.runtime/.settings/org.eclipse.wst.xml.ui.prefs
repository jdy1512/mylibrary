org.eclipse.wst.sse.ui.custom_templates=<?xml version\="1.0" encoding\="UTF-8" standalone\="no"?><templates><template autoinsert\="true" context\="xml_all" deleted\="false" description\="spring web.xml \uCD08\uAE30 \uC124\uC815 (DispatcherServlet + EncodingFilter)" enabled\="true" name\="spring_web.xml (dispatcher_servlet, encoding_filter)">&lt;\!-- dispatcher --&gt;&\#13;\r\n  &lt;servlet&gt;&\#13;\r\n      &lt;servlet-name&gt;dispatcher&lt;/servlet-name&gt;&\#13;\r\n      &lt;servlet-class&gt;org.springframework.web.servlet.DispatcherServlet&lt;/servlet-class&gt;&\#13;\r\n      &lt;init-param&gt;&\#13;\r\n          &lt;param-name&gt;contextConfigLocation&lt;/param-name&gt;&\#13;\r\n          &lt;param-value&gt;${context_location}&lt;/param-value&gt;&\#13;\r\n      &lt;/init-param&gt;&\#13;\r\n      &lt;load-on-startup&gt;1&lt;/load-on-startup&gt;&\#13;\r\n  &lt;/servlet&gt;&\#13;\r\n  &lt;servlet-mapping&gt;&\#13;\r\n      &lt;servlet-name&gt;dispatcher&lt;/servlet-name&gt;&\#13;\r\n      &lt;url-pattern&gt;${url}&lt;/url-pattern&gt;&\#13;\r\n  &lt;/servlet-mapping&gt;&\#13;\r\n  &\#13;\r\n  &lt;\!-- filter --&gt;&\#13;\r\n  &lt;filter&gt;&\#13;\r\n      &lt;filter-name&gt;encoding&lt;/filter-name&gt;&\#13;\r\n      &lt;filter-class&gt;org.springframework.web.filter.CharacterEncodingFilter&lt;/filter-class&gt;&\#13;\r\n      &lt;init-param&gt;&\#13;\r\n          &lt;param-name&gt;encoding&lt;/param-name&gt;&\#13;\r\n          &lt;param-value&gt;${encoding}&lt;/param-value&gt;&\#13;\r\n      &lt;/init-param&gt;&\#13;\r\n  &lt;/filter&gt;&\#13;\r\n  &lt;filter-mapping&gt;&\#13;\r\n      &lt;filter-name&gt;encoding&lt;/filter-name&gt;&\#13;\r\n      &lt;url-pattern&gt;${url}&lt;/url-pattern&gt;&\#13;\r\n  &lt;/filter-mapping&gt;</template></templates>
